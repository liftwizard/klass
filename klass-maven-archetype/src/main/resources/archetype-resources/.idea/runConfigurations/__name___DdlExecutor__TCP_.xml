<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="${name} DdlExecutor (TCP)" type="Application" factoryName="Application" singleton="true">
    <option name="MAIN_CLASS_NAME" value="${package}.reladomo.ddl.executor.${name}DdlExecutor" />
    <module name="stackoverflow-reladomo-ddl-executor" />
    <option name="VM_PARAMETERS" value="-Dconfig.resource=tcp.conf" />
    <option name="WORKING_DIRECTORY" value="file://$PROJECT_DIR$" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="${package}.reladomo.ddl.executor.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <method />
  </configuration>
</component>
