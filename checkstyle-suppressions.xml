<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>

    <suppress files="[\\/]generated-sources[\\/]" checks="[a-zA-Z0-9]*" />
    <suppress files="[\\/]ReladomoOperationCompilerTest.java" checks="NoWhitespaceBefore" />
    <suppress files="[\\/]TokenCategoryToCss.java" checks="UncommentedMain" />
    <suppress files="[\\/]KlassBootstrappedMetaModelApplication.java" checks="UncommentedMain" />
    <suppress files="[\\/]CoverageExampleApplication.java" checks="UncommentedMain" />
    <suppress files="[\\/]StackOverflowApplication.java" checks="UncommentedMain" />
    <suppress files=".*" checks="AbstractClassName" />
    <suppress files=".*" checks="AvoidNestedBlocks" />
    <suppress files=".*" checks="HiddenField" />
    <suppress files=".*" checks="SingleSpaceSeparator" />
    <suppress files=".*" checks="VariableDeclarationUsageDistance" />
    <suppress files=".*" checks="VisibilityModifier" />
    <suppress files=".*" id="checkASCII" />
    <suppress files="[\\/]AssociationEnd.java" checks="UnusedLocalVariable" />
    <suppress files="[\\/]SyntaxHighlightingVerificationTest.java" checks="AvoidEscapedUnicodeCharacters" />
    <suppress files="[\\/]AnsiSyntaxHighlightingTest.java" checks="AvoidEscapedUnicodeCharacters" />

</suppressions>
