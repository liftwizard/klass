        <checkstyle.version>8.18</checkstyle.version>
        <dropwizard.version>2.0.0-rc0+test8</dropwizard.version>
        <eclipse-collections.version>10.0.0.M2</eclipse-collections.version>
        <google-auth.version>0.11.0</google-auth.version>
        <google-client.version>1.25.0</google-client.version>
        <google.auto.version>1.0-rc5</google.auto.version>
        <graphql-java.version>11.0</graphql-java.version>
        <h2.version>1.4.199</h2.version>
        <hamcrest.version>1.3</hamcrest.version>
        <jaxrs.version>2.1.5</jaxrs.version>
        <jsonassert.version>1.5.0</jsonassert.version>
        <junit.version>4.13-beta-2</junit.version>
        <logback.version>1.3.0-alpha4</logback.version>
        <maven-api.version>3.6.0</maven-api.version>
        <maven-plugin-annotations.version>3.6.0</maven-plugin-annotations.version>
        <maven-project.version>3.0-alpha-2</maven-project.version>
        <plexus-component-annotations.version>2.0.0</plexus-component-annotations.version>
        <plexus-utils.version>3.2.0</plexus-utils.version>
        <reflections.version>0.9.11</reflections.version>
