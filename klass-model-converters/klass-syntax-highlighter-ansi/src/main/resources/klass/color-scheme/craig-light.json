{
  "name": "craig-light",
  "description": "Craig's light color scheme matching IntelliJ preferences",
  "rules": [
    {
      "name": "foreground",
      "style": {
        "foreground": "#000000"
      }
    },
    {
      "name": "background",
      "style": {
        "background": "#ffffff"
      }
    },
    {
      "name": "blockComment",
      "style": {
        "foreground": "#8c8c8c"
      }
    },
    {
      "name": "lineComment",
      "style": {
        "foreground": "#8c8c8c"
      }
    },
    {
      "name": "errorAnnotation",
      "style": {
        "foreground": "#cc0000"
      }
    },
    {
      "name": "warningAnnotation",
      "style": {
        "foreground": "#ff8800"
      }
    },
    {
      "name": "errorCaret",
      "style": {
        "foreground": "RED"
      }
    },
    {
      "name": "warningCaret",
      "style": {
        "foreground": "YELLOW"
      }
    },
    {
      "name": "causeCaret",
      "style": {
        "foreground": "GREEN"
      }
    },
    {
      "name": "annotationCode",
      "style": {
        "foreground": "MAGENTA"
      }
    },
    {
      "name": "metadataLabel",
      "style": {
        "foreground": "CYAN"
      }
    },

    /* Keywords */
    {
      "name": "keyword",
      "style": {
