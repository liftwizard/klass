package dummy

association DoubleVersionAssociation
{
    source: ExampleSource[1..1] version owned;
    target: ExampleTarget[1..1] version owned;

    relationship this.id == ExampleTarget.id
}

class ExampleSource
    systemTemporal
{
    id    : Long key;
    number: Integer version;
}

class ExampleTarget
    systemTemporal
{
    id    : Long key;
    number: Integer version;
}
