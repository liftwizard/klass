{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://klass.cool/klass.schema.json",
  "type": "object",
  "title": "Klass Schema",
  "required": [
    "enumerations",
    "interfaces",
    "classes",
    "associations",
    "projections",
    "services"
  ],
  "properties": {
    "enumerations": {
      "$id": "#/properties/enumerations",
      "type": "array",
      "title": "Enumerations Schema",
      "items": {
        "$id": "#/properties/enumerations/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "name",
          "packageName",
          "enumerationLiterals"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/enumerations/items/properties/name",
            "type": "string",
            "title": "Name Schema",
            "examples": [
              "InheritanceType"
            ],
            "pattern": "^[A-Z][a-zA-Z0-9]*$"
          },
          "packageName": {
            "$id": "#/properties/enumerations/items/properties/packageName",
            "type": "string",
            "title": "Package Name Schema",
            "examples": [
              "klass.model.meta.domain"
            ],
            "pattern": "^[a-z]+(\\.[a-z][a-z0-9]*)*$"
          },
          "enumerationLiterals": {
            "$id": "#/properties/enumerations/items/properties/enumerationLiterals",
            "type": "array",
            "title": "Enumeration Literals Schema",
            "items": {
              "$id": "#/properties/enumerations/items/properties/enumerationLiterals/items",
              "type": "object",
              "title": "Items Schema",
              "required": [
                "name"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/enumerations/items/properties/enumerationLiterals/items/properties/name",
                  "type": "string",
                  "title": "Name Schema",
                  "examples": [
                    "TABLE_PER_SUBCLASS"
                  ],
                  "pattern": "^[A-Z][A-Z0-9]*(_[A-Z0-9]+)*$"
                },
                "prettyName": {
                  "$id": "#/properties/enumerations/items/properties/enumerationLiterals/items/properties/prettyName",
                  "type": "string",
                  "title": "Pretty Name Schema"
                }
              }
            }
          }
        }
      }
    },
    "interfaces": {
      "$id": "#/properties/interfaces",
      "type": "array",
      "title": "Interfaces Schema",
      "items": {
        "$id": "#/properties/interfaces/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "name",
          "packageName"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/interfaces/items/properties/name",
            "type": "string",
            "title": "Name Schema",
            "examples": [
              "Element"
            ],
            "pattern": "^[A-Z][a-zA-Z0-9]*$"
          },
          "packageName": {
            "$id": "#/properties/interfaces/items/properties/packageName",
            "type": "string",
            "title": "Package Name Schema",
            "examples": [
              "klass.model.meta.domain"
            ],
            "pattern": "^[a-z]+(\\.[a-z][a-z0-9]*)*$"
          },
          "primitiveProperties": {
            "$id": "#/properties/interfaces/items/properties/primitiveProperties",
            "type": "array",
            "title": "Primitive Properties Schema",
            "items": {
              "$id": "#/properties/interfaces/items/properties/primitiveProperties/items",
              "type": "object",
              "title": "Items Schema",
              "required": [
                "name",
                "primitiveType",
                "optional",
                "key",
                "id"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/interfaces/items/properties/primitiveProperties/items/properties/name",
                  "type": "string",
                  "title": "Name Schema",
                  "examples": [
                    "inferred"
                  ],
                  "pattern": "^[a-z][a-zA-Z0-9]*$"
                },
                "primitiveType": {
                  "$id": "#/properties/interfaces/items/properties/primitiveProperties/items/properties/primitiveType",
                  "type": "string",
                  "title": "Primitive Type Schema",
                  "enum": [
                    "Integer",
                    "Long",
                    "Double",
                    "Float",
                    "Boolean",
                    "String",
                    "Instant",
                    "LocalDate",
                    "TemporalInstant",
                    "TemporalRange"
                  ]
                },
                "optional": {
                  "$id": "#/properties/interfaces/items/properties/primitiveProperties/items/properties/optional",
                  "type": "boolean",
                  "title": "Optional Schema",
                  "default": false
                },
                "key": {
                  "$id": "#/properties/interfaces/items/properties/primitiveProperties/items/properties/key",
                  "type": "boolean",
                  "title": "Key Schema",
                  "default": false
                },
                "id": {
                  "$id": "#/properties/interfaces/items/properties/primitiveProperties/items/properties/id",
                  "type": "boolean",
                  "title": "Id Schema",
                  "default": false
                }
              }
            }
          }
        }
      }
    },
    "classes": {
      "$id": "#/properties/classes",
      "type": "array",
      "title": "Classes Schema",
      "items": {
        "$id": "#/properties/classes/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "name",
          "packageName"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/classes/items/properties/name",
            "type": "string",
            "title": "Name Schema",
            "examples": [
              "Enumeration"
            ],
            "pattern": "^[A-Z][a-zA-Z0-9]*$"
          },
          "packageName": {
            "$id": "#/properties/classes/items/properties/packageName",
            "type": "string",
            "title": "Package Name Schema",
            "examples": [
              "klass.model.meta.domain"
            ],
            "pattern": "^[a-z]+(\\.[a-z][a-z0-9]*)*$"
          },
          "superInterfaces": {
            "$id": "#/properties/classes/items/properties/superInterfaces",
            "type": "array",
            "title": "Superinterfaces Schema",
            "items": {
              "$id": "#/properties/classes/items/properties/superInterfaces/items",
              "type": "object",
              "title": "Items Schema",
              "required": [
                "superInterface"
              ],
              "properties": {
                "superInterface": {
                  "$id": "#/properties/classes/items/properties/superInterfaces/items/properties/superInterface",
                  "type": "object",
                  "title": "Superinterface Schema",
                  "required": [
                    "name"
                  ],
                  "properties": {
                    "name": {
                      "$id": "#/properties/classes/items/properties/superInterfaces/items/properties/superInterface/properties/name",
                      "type": "string",
                      "title": "Name Schema",
                      "examples": [
                        "PackageableElement"
                      ],
                      "pattern": "^[A-Z][a-zA-Z0-9]*$"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "associations": {
      "$id": "#/properties/associations",
      "type": "array",
      "title": "Associations Schema",
      "items": {
        "$id": "#/properties/associations/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "name",
          "packageName",
          "associationEnds",
          "criteria"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/associations/items/properties/name",
            "type": "string",
            "title": "Name Schema",
            "examples": [
              "EnumerationHasLiterals"
            ],
            "pattern": "^[A-Z][a-zA-Z0-9]*$"
          },
          "packageName": {
            "$id": "#/properties/associations/items/properties/packageName",
            "type": "string",
            "title": "Package Name Schema",
            "examples": [
              "klass.model.meta.domain"
            ],
            "pattern": "^[a-z]+(\\.[a-z][a-z0-9]*)*$"
          },
          "associationEnds": {
            "$id": "#/properties/associations/items/properties/associationEnds",
            "type": "array",
            "title": "Association Ends Schema",
            "items": {
              "$id": "#/properties/associations/items/properties/associationEnds/items",
              "type": "object",
              "title": "Items Schema",
              "required": [
                "name",
                "multiplicity",
                "resultType"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/associations/items/properties/associationEnds/items/properties/name",
                  "type": "string",
                  "title": "Name Schema",
                  "examples": [
                    "enumerationProperty"
                  ],
                  "pattern": "^[a-z][a-zA-Z0-9]*$"
                },
                "multiplicity": {
                  "$id": "#/properties/associations/items/properties/associationEnds/items/properties/multiplicity",
                  "type": "string",
                  "title": "Multiplicity Schema",
                  "enum": [
                    "0..1",
                    "1..1",
                    "0..*",
                    "1..*"
                  ]
                },
                "resultType": {
                  "$id": "#/properties/associations/items/properties/associationEnds/items/properties/resultType",
                  "type": "object",
                  "title": "Result Type Schema",
                  "required": [
                    "name"
                  ],
                  "properties": {
                    "name": {
                      "$id": "#/properties/associations/items/properties/associationEnds/items/properties/resultType/properties/name",
                      "type": "string",
                      "title": "Name Schema",
                      "examples": [
                        "EnumerationProperty"
                      ],
                      "pattern": "^[A-Z][a-zA-Z0-9]*$"
                    }
                  }
                }
              }
            }
          },
          "criteria": {
            "$id": "#/properties/associations/items/properties/criteria",
            "type": "object",
            "title": "Criteria Schema",
            "required": [
              "_type"
            ],
            "properties": {
              "_type": {
                "$id": "#/properties/associations/items/properties/criteria/properties/_type",
                "type": "string",
                "title": "_type Schema",
                "enum": [
                  "OperatorCriteria",
                  "AndCriteria",
                  "OrCriteria"
                ]
              },
              "operator": {
                "$id": "#/properties/associations/items/properties/criteria/properties/operator",
                "type": "string",
                "title": "Operator Schema",
                "enum": [
                  "==",
                  "!=",
                  "<",
                  ">",
                  "<=",
                  ">=",
                  "in",
                  "contains",
                  "startsWith",
                  "endsWith"
                ]
              },
              "sourceExpressionValue": {
                "$id": "#/properties/associations/items/properties/criteria/properties/sourceExpressionValue",
                "type": "object",
                "title": "Source Expression Value Schema",
                "required": [
                  "_type",
                  "klass",
                  "dataTypeProperty"
                ],
                "properties": {
                  "_type": {
                    "$id": "#/properties/associations/items/properties/criteria/properties/sourceExpressionValue/properties/_type",
                    "type": "string",
                    "title": "_type Schema",
                    "enum": [
                      "ThisMemberReferencePath"
                    ]
                  },
                  "klass": {
                    "$id": "#/properties/associations/items/properties/criteria/properties/sourceExpressionValue/properties/klass",
                    "type": "object",
                    "title": "Klass Schema",
                    "required": [
                      "name"
                    ],
                    "properties": {
                      "name": {
                        "$id": "#/properties/associations/items/properties/criteria/properties/sourceExpressionValue/properties/klass/properties/name",
                        "type": "string",
                        "title": "Name Schema",
                        "examples": [
                          "EnumerationProperty"
                        ],
                        "pattern": "^[A-Z][a-zA-Z0-9]*$"
                      }
                    }
                  },
                  "dataTypeProperty": {
                    "$id": "#/properties/associations/items/properties/criteria/properties/sourceExpressionValue/properties/dataTypeProperty",
                    "type": "object",
                    "title": "Data Type Property Schema",
                    "required": [
                      "name"
                    ],
                    "properties": {
                      "name": {
                        "$id": "#/properties/associations/items/properties/criteria/properties/sourceExpressionValue/properties/dataTypeProperty/properties/name",
                        "type": "string",
                        "title": "Name Schema",
                        "examples": [
                          "enumerationName"
                        ],
                        "pattern": "^[a-z][a-zA-Z0-9]*$"
                      }
                    }
                  }
                }
              },
              "targetExpressionValue": {
                "$id": "#/properties/associations/items/properties/criteria/properties/targetExpressionValue",
                "type": "object",
                "title": "Target Expression Value Schema",
                "required": [
                  "_type",
                  "klass",
                  "dataTypeProperty"
                ],
                "properties": {
                  "_type": {
                    "$id": "#/properties/associations/items/properties/criteria/properties/targetExpressionValue/properties/_type",
                    "type": "string",
                    "title": "_type Schema",
                    "enum": [
                      "TypeMemberReferencePath"
                    ]
                  },
                  "klass": {
                    "$id": "#/properties/associations/items/properties/criteria/properties/targetExpressionValue/properties/klass",
                    "type": "object",
                    "title": "Klass Schema",
                    "required": [
                      "name"
                    ],
                    "properties": {
                      "name": {
                        "$id": "#/properties/associations/items/properties/criteria/properties/targetExpressionValue/properties/klass/properties/name",
                        "type": "string",
                        "title": "Name Schema",
                        "examples": [
                          "Enumeration"
                        ],
                        "pattern": "^[A-Z][a-zA-Z0-9]*$"
                      }
                    }
                  },
                  "dataTypeProperty": {
                    "$id": "#/properties/associations/items/properties/criteria/properties/targetExpressionValue/properties/dataTypeProperty",
                    "type": "object",
                    "title": "Data Type Property Schema",
                    "required": [
                      "name"
                    ],
                    "properties": {
                      "name": {
                        "$id": "#/properties/associations/items/properties/criteria/properties/targetExpressionValue/properties/dataTypeProperty/properties/name",
                        "type": "string",
                        "title": "Name Schema",
                        "examples": [
                          "name"
                        ],
                        "pattern": "^[a-z][a-zA-Z0-9]*$"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "projections": {
      "$id": "#/properties/projections",
      "type": "array",
      "title": "Projections Schema",
      "items": {
        "$id": "#/properties/projections/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "_type",
          "name",
          "packageName",
          "children"
        ],
        "properties": {
          "_type": {
            "$id": "#/properties/projections/items/properties/_type",
            "type": "string",
            "title": "_type Schema",
            "examples": [
              "ServiceProjection"
            ],
            "pattern": "^[A-Z][a-zA-Z0-9]*$"
          },
          "name": {
            "$id": "#/properties/projections/items/properties/name",
            "type": "string",
            "title": "Name Schema",
            "examples": [
              "EnumerationProjection"
            ],
            "pattern": "^[A-Z][a-zA-Z0-9]*$"
          },
          "packageName": {
            "$id": "#/properties/projections/items/properties/packageName",
            "type": "string",
            "title": "Package Name Schema",
            "examples": [
              "klass.model.meta.domain"
            ],
            "pattern": "^[a-z]+(\\.[a-z][a-z0-9]*)*$"
          },
          "children": {
            "$id": "#/properties/projections/items/properties/children",
            "type": "array",
            "title": "Children Schema",
            "items": {
              "$id": "#/properties/projections/items/properties/children/items",
              "type": "object",
              "title": "Items Schema",
              "required": [
                "_type",
                "name"
              ],
              "properties": {
                "_type": {
                  "$id": "#/properties/projections/items/properties/children/items/properties/_type",
                  "type": "string",
                  "title": "_type Schema",
                  "examples": [
                    "ProjectionDataTypeProperty"
                  ],
                  "pattern": "^[A-Z][a-zA-Z0-9]*$"
                },
                "name": {
                  "$id": "#/properties/projections/items/properties/children/items/properties/name",
                  "type": "string",
                  "title": "Name Schema",
                  "examples": [
                    "name"
                  ],
                  "pattern": "^[a-z][a-zA-Z0-9]*$"
                }
              }
            }
          }
        }
      }
    },
    "services": {
      "$id": "#/properties/services",
      "type": "array",
      "title": "Services Schema",
      "items": {
        "$id": "#/properties/services/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "name",
          "packageName",
          "urls"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/services/items/properties/name",
            "type": "string",
            "title": "Name Schema",
            "examples": [
              "Enumeration"
            ],
            "pattern": "^[A-Z][a-zA-Z0-9]*$"
          },
          "packageName": {
            "$id": "#/properties/services/items/properties/packageName",
            "type": "string",
            "title": "Package Name Schema",
            "examples": [
              "klass.model.meta.domain"
            ],
            "pattern": "^[a-z]+(\\.[a-z][a-z0-9]*)*$"
          },
          "urls": {
            "$id": "#/properties/services/items/properties/urls",
            "type": "array",
            "title": "Urls Schema",
            "items": {
              "$id": "#/properties/services/items/properties/urls/items",
              "type": "object",
              "title": "Items Schema",
              "required": [
                "url",
                "services"
              ],
              "properties": {
                "url": {
                  "$id": "#/properties/services/items/properties/urls/items/properties/url",
                  "type": "string",
                  "title": "Url Schema",
                  "examples": [
                    "/meta/enumeration"
                  ],
                  "pattern": "^(.*)$"
                },
                "services": {
                  "$id": "#/properties/services/items/properties/urls/items/properties/services",
                  "type": "array",
                  "title": "Services Schema",
                  "items": {
                    "$id": "#/properties/services/items/properties/urls/items/properties/services/items",
                    "type": "object",
                    "title": "Items Schema",
                    "required": [
                      "verb",
                      "serviceMultiplicity",
                      "projection",
                      "queryCriteria"
                    ],
                    "properties": {
                      "verb": {
                        "$id": "#/properties/services/items/properties/urls/items/properties/services/items/properties/verb",
                        "type": "string",
                        "title": "Verb Schema",
                        "enum": [
                          "GET",
                          "POST",
                          "PUT",
                          "PATCH",
                          "DELETE"
                        ]
                      },
                      "serviceMultiplicity": {
                        "$id": "#/properties/services/items/properties/urls/items/properties/services/items/properties/serviceMultiplicity",
                        "type": "string",
                        "title": "Service Multiplicity Schema",
                        "enum": [
                          "one",
                          "many"
                        ]
                      },
                      "projection": {
                        "$id": "#/properties/services/items/properties/urls/items/properties/services/items/properties/projection",
                        "type": "object",
                        "title": "Projection Schema",
                        "required": [
                          "name"
                        ],
                        "properties": {
                          "name": {
                            "$id": "#/properties/services/items/properties/urls/items/properties/services/items/properties/projection/properties/name",
                            "type": "string",
                            "title": "Name Schema",
                            "examples": [
                              "EnumerationSummaryProjection"
                            ],
                            "pattern": "^[A-Z][a-zA-Z0-9]*$"
                          }
                        }
                      },
                      "queryCriteria": {
                        "$id": "#/properties/services/items/properties/urls/items/properties/services/items/properties/queryCriteria",
                        "type": "object",
                        "title": "Query Criteria Schema",
                        "required": [
                          "_type"
                        ],
                        "properties": {
                          "_type": {
                            "$id": "#/properties/services/items/properties/urls/items/properties/services/items/properties/queryCriteria/properties/_type",
                            "type": "string",
                            "title": "_type Schema",
                            "examples": [
                              "AllCriteria"
                            ],
                            "pattern": "^[A-Z][a-zA-Z0-9]*$"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
