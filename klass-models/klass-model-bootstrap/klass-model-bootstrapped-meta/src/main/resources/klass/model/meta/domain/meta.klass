    },
}

service Association
{
    /api/meta/association/{associationName: String[1..1]}
        GET
        {
            multiplicity: one;
            criteria    : this.name == associationName;
            projection  : AssociationReadProjection;
        }
    /api/meta/association
        GET
        {
            multiplicity: many;
            criteria    : all;
            projection  : AssociationReadProjection;
        }
}
