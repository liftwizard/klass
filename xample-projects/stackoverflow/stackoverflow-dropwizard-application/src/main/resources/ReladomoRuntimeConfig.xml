        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/goldmansachs/reladomo/master/reladomo/src/main/xsd/mithraruntime.xsd">

    <ConnectionManager className="cool.klass.reladomo.connectionmanager.h2.H2ConnectionManager">

        <MithraObjectConfiguration cacheType="none" className="cool.klass.reladomo.simseq.ObjectSequence" />

        <MithraObjectConfiguration cacheType="partial" className="com.stackoverflow.QuestionVersion" />
        <MithraObjectConfiguration cacheType="partial" className="com.stackoverflow.Question" />
        <MithraObjectConfiguration cacheType="partial" className="com.stackoverflow.Answer" />

    </ConnectionManager>

    <PureObjects notificationIdentifier="klass.model.meta.domain">
        <MithraObjectConfiguration className="klass.model.meta.domain.Association" />
        <MithraObjectConfiguration className="klass.model.meta.domain.AssociationEnd" />
        <MithraObjectConfiguration className="klass.model.meta.domain.AssociationEndOrdering" />
