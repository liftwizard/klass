        <MithraObjectConfiguration className="klass.model.meta.domain.EnumerationLiteral" />
        <MithraObjectConfiguration className="klass.model.meta.domain.EnumerationProperty" />
        <MithraObjectConfiguration className="klass.model.meta.domain.Ordering" />
        <MithraObjectConfiguration className="klass.model.meta.domain.OrderingPart" />
        <MithraObjectConfiguration className="klass.model.meta.domain.Package" />
        <MithraObjectConfiguration className="klass.model.meta.domain.ParameterizedProperty" />
        <MithraObjectConfiguration className="klass.model.meta.domain.ParameterizedPropertyOrdering" />
        <MithraObjectConfiguration className="klass.model.meta.domain.ParameterizedPropertyParameter" />
        <MithraObjectConfiguration className="klass.model.meta.domain.PrimitiveProperty" />
    </PureObjects>
